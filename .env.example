# Bison Tracking API Configuration
# Copy this file to .env.local and update the URLs for your backend

# Base API URL for REST endpoints
VITE_API_URL=http://localhost:8000

# Server-Sent Events stream URL
VITE_STREAM_URL=http://localhost:8000/metrics/stream

# Development mode (optional)
# Set to 'true' to use mock data even when API is available
VITE_USE_MOCK_DATA=false